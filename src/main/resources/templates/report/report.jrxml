	<queryString language="SQL">
		<![CDATA[SELECT
  `gaji`.`no_gaji`,
  `gaji`.`pph21`,
  `gaji`.`password_enkrip`,
  `gaji`.`gaji_kotor`,
  `gaji`.`jumlah_potongan`,
  `gaji`.`gaji_bersih`,
  `gaji`.`tanggal`,
  `gaji`.`pegawai_nip`,
  `pegawai`.`nama_pegawai`,
  `pegawai`.`nomor_rekening`
FROM
  `gaji`
  INNER JOIN `pegawai` ON `gaji`.`pegawai_nip` = `pegawai`.`nip`
WHERE 
	`pegawai`.`nip` = 1123212]]>
	</queryString>
